# Function_And_Arrays
# Find element in an array

import java.io.*;
import java.util.*;

public class Main{

public static void main(String[] args) throws Exception {
    // write your code here
    Scanner scn = new Scanner(System.in);
    int n= scn.nextInt();
    int[] arr = new int[n];
    for(int i=0; i<n; i++)
    {
        arr[i] = scn.nextInt();
    }
    int ele = scn.nextInt();
    int ans =-1;
    for(int i=0; i<n; i++) {
        if(ele == arr[i]) {
        ans = i;
        break;
        }
    }
    System.out.println(ans);
 }
}
